// function moveAdDiv(from, to)
// moves the SCRIPT child node from the source DIV to the destination DIV
// then shows it
function moveAdDiv(from, to) {
                var phDiv = document.getElementById(from);
                var adDiv = document.getElementById(to);
                for (index = phDiv.childNodes.length - 1; index >= 0; index--)
                {
                                var element = phDiv.childNodes[index];
                                if ((element.nodeName == 'SCRIPT') || (element.nodeName == 'NOSCRIPT'))
                                                phDiv.removeChild(element);
                }
                phDiv.parentNode.removeChild(phDiv);
                adDiv.parentNode.replaceChild(phDiv, adDiv);
                phDiv.style.display = 'block';
}
ord = window.ord || Math.floor(Math.random()*1E16);
var NS_i=0;
</script>
