      $contentWindow('body').append('<div class="video-overlay"></div>');
      $contentWindow('body').append(video.render().el);
      return video.play();
    };

    return VideoModalObserver;

  })();

}).call(this);
(function() {

  CS.Views.Slide = Backbone.View.extend({
    initialize: function() {
      _.bindAll(this, 'render', 'addAll', 'addOne');
      return this.collection = this.model.get('slide_views');
    },
    render: function() {
      this.$el.html('');
      this.addAll();
      return this;
    },
    addAll: function() {
      return this.collection.forEach(this.addOne);
    },
    addOne: function(view) {
      return this.$el.append(view.render().el);
    }
  });

}).call(this);
(function() {
  var _base;

  (_base = CS.Views).Navigation || (_base.Navigation = {});

  CS.Views.Navigation.MenuItem = Backbone.View.extend({
    tagName: 'li',
    template: _.template('<a href="<%= url %>" title=\'<%= title %>\'><%= number %></a>'),
    initialize: function() {
      _.bindAll(this, 'render');
      return this.model.on('change', this.render);
    },
    render: function() {
      var renderedContent;
      renderedContent = this.$el.html(this.template(this.model.toJSON()));
      this.$el.addClass("exercise-" + this.model.cid).addClass("activity-" + (this.model.get('activity')));
      this.$el.data("completed", this.model.get('completed'));
      return this;
    }
  });

