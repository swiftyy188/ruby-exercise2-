(function() {

  CS.Views || (CS.Views = {});

  CS.Views.VideoModal = Backbone.View.extend({
    className: 'video video-modal',
    tagName: 'p',
    initialize: function(options) {
      this.currentExercise = options.currentExercise;
      return _.bindAll(this, 'render', 'play');
    },
    play: function(e) {
      var _this = this;
      _.delay(function() {
        return $contentWindow('body').addClass('is-video-playing');
      }, 100);
      this.video().attr('controls', 'controls').bind('timeupdate', function() {
        if (_this.video().get(0).ended) {
          return _this.hideVideo();
        }
      }).get(0).play();
      $contentWindow('.video-close, .video-overlay').on('click', function(e) {
        e.preventDefault();
        return _this.hideVideo();
      });
      return $('.play-video').on('click', function() {
        $('video')[0].play();
        return $(this).hide();
      });
    },
    video: function() {
      return $contentWindow('.video-modal video');
    },
    hideVideo: function() {
      _.delay(function() {
        return $contentWindow('body').removeClass('is-video-playing');
      }, 100);
      return _.delay(function() {
        $contentWindow('.video-modal').remove();
        return $contentWindow('.video-overlay').remove();
      }, 1000);
    },
    template: _.template('<a href=\'#\' class=\'video-close\'>x</a>\n<video preload=\'none\' class=\'responsive\' controls=\'controls\' style=\'top:auto; right:auto; left: auto; right: auto;\' <% if(video.thumbnail_url) { %> poster="<%= video.thumbnail_url %>"><% } %>\n\n  <% _.each(video.files, function(file) { %>\n    <source src=\'<%= file.url %>\' type=\'<%= file.type %>\'<% if(file.quality) { %> data-quality="<%= file.quality %>"<% } %> />\n  <% }) %>\n\n</video>'),
    render: function() {
      var video;
      video = this.currentExercise.previousVideo();
      this.$el.html(this.template(video.toJSON()));
      return this;
    }
  });

}).call(this);
(function() {
  var _base;

  (_base = CS.course.views).activities || (_base.activities = {});

  CS.course.views.activities.DomChallenge = Backbone.View.extend({
    className: 'challenge activity',
    events: {
      'click .next': 'next'
    },
    template: _.template('<a href=\'#\' class=\'next\'>Continue</a>'),
    conditionTemplate: _.template('var interval_id;\n\nfunction check_condition() {\n  if (<%= condition %>) {\n    clearInterval(interval_id);\n    callback();\n  }\n}\n\ninterval_id = setInterval(function() { check_condition() }, 500);\n'),
    consoleTemplate: _.template('(function(obj) {\n  var replacedFunction = obj.<%= method %>;\n  obj.<%= method %> = function() {\n    var args = Array.prototype.slice.call(arguments);\n    if (args == "<%= arguments %>") {\n      window.top.expectedConsoleCallCount--;\n      if (window.top.expectedConsoleCallCount == 0) {\n        callback();\n      }\n      obj.<%= method %> = replacedFunction;\n    }\n    return replacedFunction.apply(obj, args);\n  };\n  window.resetFunction = function() {\n    obj.<%= method %> = replacedFunction;\n  };\n}(<%= object %>));'),
    devToolsTemplate: _.template('expectedDevToolsEvent = "<%= event %>";\nexpectedDevToolsData = <%= JSON.stringify(data) %>;'),
    initialize: function() {
      var controllerClass;
      _.bindAll(this, 'render', 'next');
      controllerClass = CS.Settings.controller('domChallengeExercise');
      return this.controller = new controllerClass(this.model, this);
    },
    render: function() {
      var render;
      render = this.model.get('resources').at(0).get('render');
      if (this.lastInChain()) {
        this.$el.html(this.resolveTemplate()(this.model.toJSON()));
      }
      this.$el.find('div').html(render.html);
      this.showCalandar();
      return this;
    },
    showCalandar: function() {
      return $(contentWindow.document.getElementById('calendar')).show();
    },
    setupChallenge: function() {
      var render;
      render = this.model.get('resources').at(0).get('render');
      this.setLocalStorage(render.localStorage);
      this.injectJavascript(this.renderedJavascript(render));
      return this;
    },
    resolveTemplate: function() {
      return CS.Settings.template('activities/domChallenge') || this.template;
    },
    next: function(e) {
      var submission;
      submission = new CS.Models.Submission({
        model: this.model,
        to_data: {}
      });
      return CS.triggerAll(["exercise:submit", "exercise:" + this.model.cid + ":submit"], submission, e);
    },
    lastInChain: function() {
      return CS.Settings.get('exercises').last().cid === this.model.cid;
    },
    setLocalStorage: function(data) {
      return localStorage.setItem('events', this.parseLocalStorage(data));
    },
    parseLocalStorage: function(data) {
      var parsed_data,
        _this = this;
      parsed_data = JSON.parse(data);
      $.each(parsed_data, function(index, calendar_event) {
        return parsed_data[index].date = new Date(new Date().getTime() + (24 * 60 * 60 * 1000 * calendar_event.date));
      });
      return JSON.stringify(parsed_data);
    },
    injectJavascript: function(javascript) {
      var _this = this;
      window.top["eval"](javascript);
      return window.top.callback = function() {
        return CS.trigger("exercise:" + _this.model.cid + ":successful", _this.model);
      };
    },
    renderedJavascript: function(options) {
      if (options.setupState) {
        window.top["eval"](options.setupState);
      }
      if (options.javascript) {
        return options.javascript;
      } else if (options.condition) {
        return this.conditionTemplate({
          condition: options.condition
        });
      } else if (options.console) {
        return this.buildConsoleTemplate(options.console);
      } else if (options.devTools) {
        CS.trigger('extension:enable');
        return this.devToolsTemplate({
          event: options.devTools.action,
          data: options.devTools.data
        });
      }
    },
    buildConsoleTemplate: function(options) {
      var callCount, opts, template;
      callCount = options.length || 1;
      template = callCount > 1 ? ((function() {
        var _i, _len, _results;
        _results = [];
        for (_i = 0, _len = options.length; _i < _len; _i++) {
          opts = options[_i];
          _results.push(this.consoleTemplate(opts));
        }
        return _results;
      }).call(this)).join("\n") : this.consoleTemplate(options);
      return template += "\nwindow.top.expectedConsoleCallCount = " + callCount + ";";
    }
  });

}).call(this);
(function() {

  window.contentWindow = window.top;

  window.$contentWindow = window.top.$;

  CS.config({
    course_name: 'Try Dev Tools',
    controllers: {
      videoExercise: CS.Controllers.Exercises.VideoController,
      domChallengeExercise: CS.course.controllers.DomChallengeController
    },
    views: {
      videoExercise: CS.Views.ExerciseTypes.Exercise,
      videoDetails: CS.Views.Exercise.Details,
      videoActivity: CS.Views.Activities.Video,
      domChallengeDetails: CS.Views.Exercise.Details,
      domChallengeExercise: CS.Views.ExerciseTypes.Exercise,
      domChallengeActivity: CS.course.views.activities.DomChallenge,
      score: CS.Views.Hidden
    },
    observers: {
      exercise: CS.course.observers.ExerciseObserver,
      extension: CS.course.observers.ExtensionObserver,
      transition: CS.course.observers.TransitionObserver,
      sidebar: CS.course.observers.SidebarObserver,
      video: CS.course.observers.VideoModalObserver
    },
    setup: function(options) {
      var active_exercises, current, exercise, exercises, exercisesView, labs, navView, sidebars, views;
      CS.Settings.set('position', options.position);
      CS.Settings.set('wrapper', options.wrapper);
      CS.Settings.set('user', new CS.Models.User(options.current_user));
      labs = new CS.Collections.Labs(options.labs);
      CS.Settings.set('labs', labs);
      exercise = CS.Settings.exercise(options.position.lab_id, options.position.exercise_id);
      exercises = labs.getLab(options.position.lab_id).get('exercises');
      active_exercises = exercises.filter(function(exercise) {
        return exercise.get('active') === true;
      });
      current = active_exercises.filter(function(exercise) {
        return exercise.get('completed') !== true;
      })[0];
      if (!current) {
        current = exercise;
      }
      CS.Settings.set('exercises', new CS.Collections.Exercises(active_exercises));
      CS.Settings.set('currentExercise', new CS.Models.CurrentExercise({
        exercise: current
      }));
      sidebars = [];
      navView = CS.Settings.view('navigation');
      sidebars.push(new navView({
        collection: labs
      }));
      views = [];
      navView = CS.Settings.view('sidebar');
      views.push(new navView({
        collection: sidebars
      }));
      exercisesView = CS.Settings.view('exercises');
      views.push(new exercisesView({
        collection: CS.Settings.get('exercises')
      }));
      return CS.Settings.set('slide_views', views);
    }
  });

  $(function() {
    return _.delay(function() {
      return $('a[rel="tooltip"]').tooltip({
        animation: false
      });
    }, 100);
  });

}).call(this);
(function() {

  CS.template('exercise/details', _.template('<a name="#<%= level_number %>-<%= number %>"></a>\n<div class=\'challenge-objective\'>\n  <div class="instructions">\n    <%= instructions %>\n  </div>\n</div>\n<div class="success_message">\n  <%= success_message %>\n</div>'));

  CS.template('activities/video', _.template('<div class="video-inner">\n  <div class=\'blackboard--video\'></div>\n\n  <a href="#" class="next"><%= nextText %></a>\n  <a href="#" class="skip"><%= skipText %></a>\n</div>\n\n<div class="video-extras">\n  <% if(link) { %>\n    <div class="video-links">\n      <h4>Video Reference Links<h4>\n      <ul>\n        <li><a target=_blank href="<%= link.url %>"><%= link.text %></a></li>\n      </ul>\n    </div>\n  <% } %>\n\n  <ul class="video-downloads">\n    <% _.each(video.files, function(file) { %>\n      <li class=\'type-<%= file.type.replace(\'/\', \'-\') %>\'><a href="<%= file.url %>">Download <%= file.type.replace(\'video/\', \'\') %><% if(file.quality) { %> (<%= file.quality %>)<% } %></a></li>\n    <% }) %>\n  </ul>\n</div>'));

  CS.Views.Activities.Video.prototype.events = {
    'click .next': 'next',
    'click .skip': 'skip'
  };

  CS.Views.Activities.Video.prototype.skip = function(e) {
    e.preventDefault();
    return window.location = this.model.next_video_url();
  };

  CS.Views.Exercise.Details.prototype.render = function(e) {
    this.$el.html(this.resolveTemplate()(this.model.toJSON()));
    this.$el.find('p').prepend('<i class="icn"></i>');
    return this;
  };

}).call(this);
(function() {

  CS.Models.Exercise.prototype.next_video_url = function() {
    var current_exercise, next_chapter, next_video, remaining_exercises;
    current_exercise = parseInt(this.get('number'));
    remaining_exercises = _.rest(this.get('lab').get('exercises').models, current_exercise);
    next_video = _.filter(remaining_exercises, function(exercise) {
      return exercise.get('activity') === 'video';
    })[0];
    if (next_video) {
      return next_video.get('url');
    } else if ((parseInt(this.get('lab').id)) === 7) {
      return "/chapters/1/challenges/1";
    } else {
      next_chapter = parseInt(this.get('lab').id + 1);
      return "/chapters/" + next_chapter + "/challenges/1";
    }
  };

  CS.Observers.CorrectSubmissionObserver.prototype.correct = function(options) {
    var exercise;
    exercise = options.response.currenct_exercise;
    if (exercise.completed_course) {
      return contentWindow.location.href = '/wrap_up';
    } else if (exercise.completed_level) {
      return contentWindow.location.href = '/chapters/' + exercise.level_number + '/wrap_up';
    } else {
      return CS.trigger('exercise:transition', this.currentExercise(options.response.currenct_exercise), this.nextExercise(options.response.next_exercise));
    }
  };

  CS.Models.Exercise.prototype.previousVideo = function() {
    var current_exercise, previous_exercises, previous_video;
    current_exercise = parseInt(this.get('number'));
    previous_exercises = _.first(this.get('lab').get('exercises').models, current_exercise);
    return previous_video = _.filter(previous_exercises.reverse(), function(exercise) {
      return exercise.get('activity') === 'video';
    })[0];
  };

  CS.Models.Exercise.prototype.updateState = function() {
    var _this = this;
    return $(contentWindow).load(function() {
      return contentWindow["eval"](_this.get('correctState'));
    });
  };

  CS.Models.Exercise.prototype.calculatePoints = function() {
    var _this = this;
    return _.reduce(CS.Settings.get('exercises').models, function(memo, exercise) {
      if (exercise.get('number') < _this.get('number')) {
        return memo + exercise.get('points');
      } else {
        return memo + 0;
      }
    }, 0);
  };

}).call(this);
(function() {



}).call(this);
