    extensionTemplate: _.template('<div class=\'modal-body\'>\n  <p>In order to complete the DevTools course challenges, you need to install the Discover DevTools Companion extension.</p>\n<div class=\'modal-footer\'>\n  <a class=\'btn install-extension\' href="#">Install Chrome Extension</a>\n  <p class=\'skip-install\'>Or, you\'re welcome to skip the install now and just watch the <a class=\'next-video skip\' href="#">course videos</a>. Feel free to install the extension when you want to complete the course challenges.</p>\n  <p class="disclaimer">Privacy Notice: On the discover-devtools.codeschool.com domain, <a href=\'/faq\'>some of your actions within Chrome DevTools</a> will be logged by this extension and passed to us, Code School, so we can reward your progress as you complete the challenges. This app will ask for broad permissions, <a href=\'/faq#extension-access\'>here\'s why</a>.\n</div>'),
    chromeTemplate: _.template('<div class=\'modal-header\'>\n  <h2> Install Google Chrome</h2>\n</div>\n<div class=\'modal-body\'>\n  <p>In order to use this course, you need to install Google Chrome.</p>\n</div>\n<div class=\'modal-footer\'>\n  <a class=\'btn\' href="http://google.com/chrome">Install Google Chrome</a>\n</div>'),
    updateTemplate: _.template('<div class=\'modal-header\'>\n  <h2> Update Google Chrome</h2>\n</div>\n<div class=\'modal-body\'>\n  <p>In order to use this course, you need to update Google Chrome. This course\n  requires at least Version 24 of the browser.</p>\n</div>\n<div class=\'modal-footer\'>\n  <a class=\'btn\' href="http://google.com/chrome">Install Google Chrome</a>\n</div>'),
    resolveTemplate: function() {
      if (/chrom(e|ium)/.test(navigator.userAgent.toLowerCase())) {
        return this.extensionTemplate();
      } else {
        return this.chromeTemplate();
      }
    },
    render: function() {
      this.$el.html(this.resolveTemplate());
      return this;
    }
  });

