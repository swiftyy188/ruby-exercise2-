(function() {
  var _base;

  (_base = CS.Views).Navigation || (_base.Navigation = {});

  CS.Views.Navigation.SidebarChapter = Backbone.View.extend({
    tagName: 'li',
    template: _.template('<h3><a title="<%= name %>" href="<%= lab_url %>" rel="tooltip" data-placement="bottom">\n  <%= number %><b>. <%= name %></b>\n</a></h3>\n<ol class="sidebar-sections"></ol>'),
    initialize: function() {
      _.bindAll(this, 'render', 'addAll', 'addOne');
      this.model.bind('change', this.render, this);
      return this.collection = this.model.get('exercises');
    },
    render: function() {
      this.$el.html(this.template(this.model.toJSON()));
      this.$el.removeClass('inactive').removeClass('active');
      if (this.collection.length > 0) {
        this.addAll();
        this.$el.addClass('active');
      }
      if (this.model.get('complete')) {
        this.$el.addClass('inactive');
      }
      return this;
    },
    addAll: function() {
      var exercise, sections;
      sections = _.compact(_.uniq((function() {
        var _i, _len, _ref, _results;
        _ref = this.collection.models;
        _results = [];
        for (_i = 0, _len = _ref.length; _i < _len; _i++) {
          exercise = _ref[_i];
          _results.push(exercise.get('section'));
        }
        return _results;
      }).call(this)));
      return sections.forEach(this.addOne);
    },
    addOne: function(section) {
      var sectionMenu, viewClass;
      viewClass = CS.Settings.view('navigationSection');
      sectionMenu = new viewClass({
        section: section,
        collection: new CS.Collections.Exercises(this.exercisesForSection(section))
      });
      return this.$el.find('.sidebar-sections').append(sectionMenu.render().el);
    },
    exercisesForSection: function(section) {
      var _this = this;
      return _.filter(this.collection.models, function(exercise) {
        return exercise.get('section') === section;
      });
    }
  });

}).call(this);
